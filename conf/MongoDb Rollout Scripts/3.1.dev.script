use d-hrsifu-leave
db.leavesetting.update({},{ $set: {"aprmthd": "Only manager can approve leave request" }}, false, true)

# Backup leave approver id
db.leave.find().snapshot().forEach(
    function (elem) {
    	db.leave.update({_id: elem._id}, {$push: { "wf.apridbackup": elem.wf.aprid }});
    }
);

# Backup leave approver name
db.leave.find().snapshot().forEach(
    function (elem) {
    	db.leave.update({_id: elem._id}, {$push: { "wf.aprnbackup": elem.wf.aprn }});
    }
);

# Update on approved leave.
# Create List(aprbyid), List(aprbyn).
db.leave.find({"wf.s":"Approved"}).snapshot().forEach(
    function (elem) {
    	elem.wf.aprbyid = elem.wf.apridbackup;
    	elem.wf.aprbyn = elem.wf.aprnbackup;
    	db.leave.save(elem);
    }
);

# Update on rejected leave.
# Create rjtbyid, rjtbyn.
db.leave.find({"wf.s":"Rejected"}).snapshot().forEach(
    function (elem) {
    	elem.wf.rjtbyid = elem.wf.aprid;
    	elem.wf.rjtbyn = elem.wf.aprn;
    	db.leave.save(elem);
    }
);

# Update on cancelled leave.
# Create cclbyid, cclbyn.
db.leave.find({"wf.s":"Cancelled"}).snapshot().forEach(
    function (elem) {
    	elem.wf.cclbyid = elem.wf.aprid;
    	elem.wf.cclbyn = elem.wf.aprn;
    	db.leave.save(elem);
    }
);

# Create List(aprid), List(aprn).
db.leave.find().snapshot().forEach(
    function (elem) {
    	delete elem.wf.aprid;
    	delete elem.wf.aprn;
    	elem.wf.aprid = elem.wf.apridbackup;
    	elem.wf.aprn = elem.wf.aprnbackup;
    	delete elem.wf.apridbackup;
    	delete elem.wf.aprnbackup;
    	db.leave.save(elem);
    }
);

db.leave.update({},{ $set: {"wf.aprmthd": "Only manager can approve leave request" }}, false, true)

